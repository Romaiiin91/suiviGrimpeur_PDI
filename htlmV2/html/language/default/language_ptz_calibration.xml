    <rotateLeftBtn type="image" minor="0" imageid="rotateButtonLeft">Rotate left</rotateLeftBtn>
    <rotateRightBtn type="image" minor="0" imageid="rotateButtonRight">Rotate right</rotateRightBtn>
    <buttonBack type="button" minor="0" buttonid="buttonBack">Back</buttonBack>
    <buttonNext type="button" minor="0" buttonid="buttonNext">Next</buttonNext>
    <buttonFinish type="button" minor="0" buttonid="buttonFinish">Finish</buttonFinish>
    <buttonCancel type="button" minor="0" buttonid="buttonCancel">Cancel</buttonCancel>
    <helpLink type="text" minor="0">Help</helpLink>
  </ptz_calibration>
