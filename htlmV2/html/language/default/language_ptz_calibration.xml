<?xml version="1.0" encoding="UTF-8"?>
<language_xml>
  <ptz_calibration>
    <calibrationText type="text" minor="0">PTZ Calibration</calibrationText>
    <pageTitle type="title" minor="0">Adjust orientation</pageTitle>
    <pageText1 type="text" minor="0">Use the buttons below to set the orientation in overview.</pageText1>
    <pageText2 type="text" minor="0">Points remaining:</pageText2>
    <ptzClick1 type="text" minor="0">Select an object inside the small circle, and click on it.&#x3C;br /&#x3E;Remember the selected object for the next step.</ptzClick1>
    <ptzClick2 type="text" minor="0">Select the same object again.</ptzClick2>
    <timedOut1 type="text" minor="0">Calibration has timed out.</timedOut1>
    <timedOut2 type="text" minor="0">Do you want to start over with calibration?</timedOut2>
    <timedOut3 type="text" minor="0">If you press 'Cancel' the calibration will be aborted.</timedOut3>
    <restartWait type="text" minor="0">Restarting calibration, please wait...</restartWait>
    <abortWait type="text" minor="0">Aborting calibration, please wait...</abortWait>
    <saveRotWait type="text" minor="0">Saving rotation, please wait...</saveRotWait>
    <initWait type="text" minor="0">Camera is initializing, please wait...</initWait>
